title = {{Executive functions, self-regulation, and chronic pain: a review}},
volume = {37},
year = {2009}
}
@article{Melzack1965,
abstract = {The nature of pain has been the subject of bitter controversy since the turn of the century. THere are currently two opposing theories of pain: (i) specificity theory, which holds that pain is a special modality like vision or hearing, "with it's own central and peripheral apparatus", and (ii) pattern theory, which maintains that the nerve impulse pattern for pain is produced by intense stimulation of non-specific receptors since "there are no specific fibers and no specific endings". Both theories desrive from earlier concepts proposed by von Frey and Goldscheider in 1894, and historically they are held to be mutally exclusive. Since it is our purpose here to propose a new theory of pain mechanisms, we shall stat explicitly at the outset where we agree and disagree with specificity and pattern theories.},
author = {Melzack, Ronald and Wall, Patrick D},
doi = {10.1016/S1082-3174(96)80062-6},
file = {:Users/sinkpoint/Documents/Mendeley Desktop/Melzack, Wall - 1965 - Pain Mechanisms A New Theory.pdf:pdf},
isbn = {00368075},
