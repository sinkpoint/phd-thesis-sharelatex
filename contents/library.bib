issn = {1936959X},
journal = {American Journal of Neuroradiology},
number = {8},
pages = {1384--1392},
pmid = {26892985},
title = {{Imaging of neurovascular compression syndromes: Trigeminal neuralgia, Hemifacial spasm, vestibular Paroxysmia, and Glossopharyngeal neuralgia}},
