keywords = {Amygdala,Amygdala: anatomy {\&} histology,Automation,Computer-Assisted,Computer-Assisted: methods,Hippocampus,Hippocampus: anatomy {\&} histology,Humans,Image Processing,Magnetic Resonance Imaging},
month = {may},
