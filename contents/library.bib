journal = {Neuroscience and Biobehavioral Reviews},
keywords = {ACC,Anterior cingulate cortex,Anterior insula,Emotion,Empathy,FMRI,MCC,MKDA,Meta-analysis,Mid-cingulate cortex,SMA,Supplementary motor area},
number = {3},
