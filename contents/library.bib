journal = {Brain : a journal of neurology},
keywords = {brain,functional imaging,headache,migraine,morphometry,vbm},
month = {jun},
number = {Pt 6},
pages = {1419--25},
title = {{Morphing voxels: the hype around structural imaging of headache patients.}},
volume = {132},
year = {2009}
}
