month = {jul},
number = {7},
pages = {1188--1206},
pmid = {22395207},
title = {{Biophysical and physiological origins of blood oxygenation level-dependent fMRI signals.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3390806{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {32},
year = {2012}
}
