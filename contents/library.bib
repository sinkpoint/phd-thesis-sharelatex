month = {oct},
number = {5},
pages = {471--83},
pmid = {16151047},
title = {{In search of the motor engram: motor map plasticity as a mechanism for encoding motor experience.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16151047},
volume = {11},
year = {2005}
