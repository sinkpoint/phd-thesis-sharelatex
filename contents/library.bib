pages = {1263--70},
title = {{Geometrically constrained two-tensor model for crossing tracts in DWI.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2000805{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {24},
