@article{Caterina2000,
abstract = {The capsaicin (vanilloid) receptor VR1 is a cation channel expressed by primary sensory neurons of the "pain" pathway. Heterologously expressed VR1 can be activated by vanilloid compounds, protons, or heat ({\textgreater}43 degrees C), but whether this channel contributes to chemical or thermal sensitivity in vivo is not known. Here, we demonstrate that sensory neurons from mice lacking VR1 are severely deficient in their responses to each of these noxious stimuli. VR1-/- mice showed normal responses to noxious mechanical stimuli but exhibited no vanilloid-evoked pain behavior, were impaired in the detection of painful heat, and showed little thermal hypersensitivity in the setting of inflammation. Thus, VR1 is essential for selective modalities of pain sensation and for tissue injury-induced thermal hyperalgesia.},
author = {Caterina, M J and Leffler, A and Malmberg, A B and Martin, W J and Trafton, J and Petersen-Zeitz, K R and Koltzenburg, M and Basbaum, A I and Julius, D},
