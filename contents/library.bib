@article{Pople2012,
abstract = {BACKGROUND: EVD catheters provide reliable and accurate means of monitoring ICP and alleviating elevated pressures via drainage of CSF. CSF infections occur in approximately 9{\%} of patients. Antibiotic impregnated (AI)-EVD catheters were developed with the goal of reducing the occurrence of EVD catheter-related CSF infections and its associated complications. OBJECTIVE: To present an international, prospective, randomized, open-label trial to evaluate infection incidence of AI versus standard EVD catheters. METHODS: Infection was defined as: 1) Proven Infection: positive CSF culture and positive gram stain or 2) Suspected Infection: (A) Positive CSF culture with no organisms identified on initial gram stain; (B) Negative CSF culture with a gram positive or negative gram stain; (C) CSF leukocytosis with a WBC/RBC CSF count {\textgreater}0.02. RESULTS: 434 patients underwent implantation of an EVD catheter. 176 patients in the AI-EVD cohort and 181 in the standard EVD catheter cohort were eligible for evaluation of infection. The two groups were similar in all clinical characteristics. Proven Infection was documented in 9 (2.5{\%}) patients [AI: 4 (2.3{\%}) vs. standard: 5 (2.8{\%}), p=1.0]. Suspected Infection was documented in 31 (17.6{\%}) patients receiving AI and 37 (20.4{\%}) patients receiving standard EVD catheters, p=0.504. Duration of time to Suspected Infection was prolonged in the AI cohort (8.8±6.1 days) compared to the standard EVD cohort (4.6±4.2 days), p=0.002. CONCLUSION: AI-EVD catheters were associated with an extremely low rate of catheter-related infections. AI catheters were not associated with risk reduction in EVD infection compared to standard catheters. Use of AI-EVD catheters is a safe option for a wide variety of patients requiring CSF drainage and monitoring, but the efficacy of AI-EVD catheters was not supported in this trial.},
author = {Pople, Ian and Poon, Wai and Assaker, Richard and Mathieu, David and Iantosca, Mark and Wang, Ernest and Zhang, Li Wei and Leung, Gilberto and Chumas, Paul and Menei, Philippe and Beydon, Laurent and Hamilton, Mark and Kamaly, Ian and Lewis, Stephen and Ning, Wang and Megerian, J Thomas and McGirt, Matthew J and Murphy, Jeffrey A and Michael, Aileen and Meling, Torstein},
doi = {10.1227/NEU.0b013e3182544e31},
issn = {1524-4040},
journal = {Neurosurgery},
month = {mar},
pmid = {22418581},
title = {{Comparison of Infection Rate with the Use of Antibiotic-Impregnated Versus Standard Extraventricular Drainage Devices (EVD): A Prospective, Randomized Controlled Trial.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22418581},
year = {2012}
}
