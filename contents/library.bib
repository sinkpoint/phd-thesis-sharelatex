file = {:Users/sinkpoint/Documents/Mendeley Desktop/Ratajczak et al. - 2016 - NRK1 controls nicotinamide mononucleotide and nicotinamide riboside metabolism in mammalian cells.pdf:pdf},
isbn = {2041-1723 (Electronic)2041-1723 (Linking)},
issn = {2041-1723},
journal = {Nature Communications},
pages = {13103},
pmid = {27725675},
title = {{NRK1 controls nicotinamide mononucleotide and nicotinamide riboside metabolism in mammalian cells}},
url = {http://www.nature.com/doifinder/10.1038/ncomms13103},
volume = {7},
year = {2016}
}
