pmid = {18055222},
title = {{Accurate prediction of V1 location from cortical folds in a surface coordinate system.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2258215{\&}tool=pmcentrez{\&}rendertype=abstract},
