month = {jun},
number = {7045},
pages = {1102--1107},
pmid = {15973409},
title = {{Invariant visual representation by single neurons in the human brain.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15973409},
volume = {435},
year = {2005}
}
