isbn = {1095-9572 (Electronic)$\backslash$r1053-8119 (Linking)},
issn = {10538119},
journal = {NeuroImage},
keywords = {Curve variation,Diffeomorphic metric mapping,Laplace-Beltrami operator,Surface alignment consistency},
number = {1},
