month = {jul},
number = {0952-3480 (Print)},
pages = {589--597},
pmid = {18041806},
title = {{Axonal injury detected by in vivo diffusion tensor imaging correlates with neurological disability in a mouse model of multiple sclerosis}},
volume = {21},
