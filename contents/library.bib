month = {aug},
number = {16},
pages = {7320--4},
pmid = {1502142},
title = {{Evolution of consciousness.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=49701{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {89},
year = {1992}
}
